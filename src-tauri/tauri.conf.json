{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "BatteryMaster",
  "version": "0.1.1",
  "identifier": "net.weero.BatteryMaster.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "center": true,
        "shadow": true,
        "title": "BatteryMaster",
        "width": 640,
        "height": 600,
        "transparent": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [],
    "icon": ["icons/icon.ico"]
  }
}
